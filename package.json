{
  "name": "@thewass/wass-lib",
  "version": "0.2.0-alpha.0",
  "license": "MIT",
  "author": "TheWass",
  "description": "This is a TS Library where I keep my commonly used, generic functions and modules consolidated and up to date.",
  "repository": {
    "type": "git",
    "url": "git@github.com-thewass:TheWass/wass-lib.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "test": "mocha '**/*.spec.ts'",
    "prepublishOnly": "npm run test && npm run clean && npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.node.js",
      "browser": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.13",
    "chai": "^5.2.1",
    "eslint": "^9.30.1",
    "mocha": "^11.7.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "@types/luxon": "^3.6.2",
    "luxon": "^3.7.1"
  }
}
