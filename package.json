{
  "name": "@thewass/wass-lib",
  "version": "0.2.2-alpha.0",
  "license": "MIT",
  "author": "TheWass",
  "description": "This is a TS Library where I keep my commonly used, generic functions and modules consolidated and up to date.",
  "repository": {
    "type": "git",
    "url": "git@github.com-thewass:TheWass/wass-lib.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./node": {
      "types": "./dist/types/index.node.d.ts",
      "import": "./dist/esm/index.node.js",
      "require": "./dist/cjs/index.node.js"
    }
  },
  "files": [
    "dist/cjs/**/!(*.tsbuildinfo)",
    "dist/esm/**/!(*.tsbuildinfo)",
    "dist/types/**/!(*.tsbuildinfo)",
    "node",
    "package.json",
    "readme.md",
    "tsconfig.json"
  ],
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build": "yarn build:clean && yarn compile && node ./scripts/gen-alias.cjs",
    "compile": "tsc -b ./tsconfig.esm.json ./tsconfig.cjs.json ./tsconfig.types.json",
    "lint": "tsc --noEmit",
    "test": "mocha '**/*.spec.ts'",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.13",
    "chai": "^5.2.1",
    "eslint": "^9.30.1",
    "mocha": "^11.7.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "@types/luxon": "^3.6.2",
    "luxon": "^3.7.1"
  }
}
